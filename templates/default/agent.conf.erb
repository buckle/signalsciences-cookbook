accesskeyid = "<%= node['bke_signalsciences']['access_key'] %>"
secretaccesskey = "<%= node['bke_signalsciences']['secret'] %>"
<% if not node['bke_signalsciences']['rpc_address'].empty? %>
rpc-address = "<%= node['bke_signalsciences']['rpc_address'] %>"
<% end %>
<% if not node['bke_signalsciences']['client_ip_header'].empty? %>
client-ip-header = "<%= node['bke_signalsciences']['client_ip_header'] %>"
<% end %>
<% node['bke_signalsciences']['debug'].each do |flag, val| %>
<% if val > 0 %>
debug-<%= flag %> = <%= val %>
<% end %>
<% end %>
